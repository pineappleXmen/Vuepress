#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vuepress-project/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vuepress-project/docs/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vuepress@2.0.0-beta.59_zuhh5cekyzsqox63zj3ccba2ny/node_modules/vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../vuepress@2.0.0-beta.59_zuhh5cekyzsqox63zj3ccba2ny/node_modules/vuepress/bin/vuepress.js" "$@"
fi
